    "pages/community/detail",
    "pages/community/collected",
    "pages/report/report",
    "pages/profile/profile",
    "pages/settings/settings"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#07c160",
    "navigationBarTitleText": "家庭菜品管理",
